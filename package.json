    "eslint": "^10.0.2",
    "husky": "^4.3.5",
    "prettier": "^2.6.0",
    "semantic-release": "^17.1.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
